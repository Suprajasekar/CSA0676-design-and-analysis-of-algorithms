def is_subset_sum(arr, n, target):
    if target == 0:
        return True
    if n == 0 and target != 0:
        return False
    if arr[n-1] > target:
        return is_subset_sum(arr, n-1, target)
    return is_subset_sum(arr, n-1, target) or is_subset_sum(arr, n-1, target-arr[n-1])

arr = [3, 34, 4, 12, 5, 2]
target_sum = 9
n = len(arr)
if is_subset_sum(arr, n, target_sum):
    print("Subset with the given sum exists")
else:
    print("No subset with the given sum")
