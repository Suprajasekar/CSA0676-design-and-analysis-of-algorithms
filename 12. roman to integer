def romanToInt(s):
    # Map of Roman numerals to their integer values
    roman_to_int = {
        'I': 1, 'V': 5, 'X': 10, 'L': 50,
        'C': 100, 'D': 500, 'M': 1000
    }

    total = 0
    prev_value = 0

    # Traverse the string from left to right
    for char in s:
        current_value = roman_to_int[char]

        # If the current value is greater than the previous value,
        # it means we need to subtract twice the previous value
        # (once because it was added earlier and once for the subtraction rule)
        if current_value > prev_value:
            total += current_value - 2 * prev_value
        else:
            total += current_value

        # Update the previous value for the next iteration
        prev_value = current_value

    return total

# Example usage
print(romanToInt("III"))    # Output: 3
print(romanToInt("IV"))     # Output: 4
print(romanToInt("IX"))     # Output: 9
print(romanToInt("LVIII"))  # Output: 58
print(romanToInt("MCMXCIV"))# Output: 1994
