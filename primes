def check_prime(num, i=2):
    if num <= 2:
        return num == 2
    if num % i == 0:
        return False
    if i * i > num:
        return True
    return check_prime(num, i + 1)

def generate_primes(n):
    if n > 1:
        generate_primes(n - 1)
        if check_prime(n):
            print(n)

n = int(input("Enter a number: "))
print(f"Prime numbers up to {n}:")
generate_primes(n)

